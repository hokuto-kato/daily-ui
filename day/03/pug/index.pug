- contentName = "Daily UI - 03"
doctype html
html(lang="ja")
	head
		meta(charset="UTF-8")
		meta(name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0")
		meta(name="description" content=contentName)
		meta(http-equiv="X-UA-Compatible" content="ie=edge")
		title !{contentName}
	body
		section.main
			h1.main__title Daily UI
			.main__img(data-img)
			.progress__container
				.progress__bar-wrap(data-progress-bar-wrap)
					.progress__bar(data-progress-bar)
				h2.progress__daily-id 03
			.progress__number(data-progress-number) 1