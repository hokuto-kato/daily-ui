- contentName = "Daily UI - 31"
doctype html
html(lang="ja")
	head
		meta(charset="UTF-8")
		meta(name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0")
		meta(name="description" content=contentName)
		meta(http-equiv="X-UA-Compatible" content="ie=edge")
		title !{contentName}
	body
		#particles-js.bg
		main.main
			h1.main__title Daily UI 31 : JELLY FISH
			.main__img
				picture
					source(srcset=require("~/img/img.webp") type="image/webp")
					img.main__img-body(src=require("~/img/img.jpg") alt="クラゲのイメージ" width="960" height="1128")