- contentName = "Daily UI - 31"
doctype html
html(lang="ja")
	head
		meta(charset="UTF-8")
		meta(name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0")
		meta(name="description" content=contentName)
		meta(http-equiv="X-UA-Compatible" content="ie=edge")
		title !{contentName}
	body
		#particles-js.bg
		main.main
			.main__img
				picture
					video.main__img-body(height="1128" loop="true" autoplay muted playsinline)
						source(src=require("~/mov/jellyfish.webm") type="video/webm")
						source(src=require("~/mov/jellyfish.mp4") type="video/mp4")